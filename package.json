{
	"name": "lilypond-formatter",
	"displayName": "LilyPond Formatter",
	"description": "LilyPond Formatter",
	"version": "0.0.1",
	"publisher": "lhl2617",
	"preview": true,
	"repository": "https://github.com/lhl2617/lilypond-formatter",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onLanguage:lilypond"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.19.6",
		"@types/vscode": "^1.51.0",
		"@typescript-eslint/eslint-plugin": "^4.0.0",
		"@typescript-eslint/parser": "^3.10.1",
		"eslint": "^7.14.0",
		"typescript": "^4.0.2"
	}
}
