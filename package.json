{
  "name": "lilypond-formatter",
  "displayName": "LilyPond Formatter",
  "description": "LilyPond Formatter",
  "version": "0.1.2",
  "publisher": "lhl2617",
  "preview": true,
  "license": "MIT",
  "icon": "assets/icon.png",
  "repository": "https://github.com/lhl2617/VSLilyPond-formatter",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:lilypond"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "LilyPond Formatter",
      "properties": {
        "lilypondFormatter.general.pathToPython": {
          "type": "string",
          "default": "python",
          "group": "General",
          "description": "Path to `python` executable. Default (`python`) assumes python is in PATH variables.",
          "scope": "resource"
        },
        "lilypondFormatter.general.reformatTimeout": {
          "type": "number",
          "default": 10000,
          "group": "General",
          "description": "Maximum timeout (in ms) for reformatting process.",
          "scope": "resource"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run lint && npm run generateDocs",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "generateDocs": "ts-node ./src/docsGenerator/docsGenerator.ts"
  },
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/node": "^14.14.37",
    "@types/vscode": "^1.34.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.22.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "vsce": "^1.87.0"
  },
  "dependencies": {
    "command-exists": "^1.2.9"
  }
}
